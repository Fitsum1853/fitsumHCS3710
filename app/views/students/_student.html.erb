<div id="<%= dom_id student %>">
  <p>
    <strong>First name:</strong>
    <%= student.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= student.last_name %>
  </p>

  <p>
    <strong>School email:</strong>
    <%= student.school_email %>
  </p>

  <p>
    <strong>Major:</strong>
    <%= student.major %>
  </p>

  <p>
    <strong>Graduation date:</strong>
    <%= student.graduation_date %>
  </p>

  <p>
    <strong>Profile Pic:</strong><br>
    <% if student.profile_pic.attached? %>
      <%= image_tag student.profile_pic.variant(resize: "150x150") %>
    <% else %>
      <%= image_tag 'https://via.placeholder.com/150' %>
    <% end %>
  </p>

</div>
